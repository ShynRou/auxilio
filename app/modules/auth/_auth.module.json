{
  "id": "auth",
  "author": "Dustin 'ShynRou' Hagemeier",
  "rights": {
    "core": true
  },
  "handler": "auth.cmd.js",
  "commands": {
    "login": {
      "handler": "./login.cmd.js",
      "options": [
        {"short": "u", "long": "username", "params": 1},
        {"short": "p", "long": "password", "params": 1}
      ]
    },
    "logout": {
      "handler": "./logout.cmd.js"
    },
    "passowrd": {
      "handler": "./password.cmd.js",
      "options": [
        {"short": "o", "long": "old", "params": 1},
        {"short": "n", "long": "new", "params": 1}
      ]
    },
    "register": {
      "group": ["admin", "auth"],
      "handler": "./register.cmd.js",
      "options": [
        {"short": "u", "long": "username", "params": 1},
        {"short": "p", "long": "password", "params": 1},
        {"short": "e", "long": "email", "params": 1}
      ]
    }
  }
}